{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/ipwa/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/ipwa/favicon.ico",
          "/ipwa/manifest.webmanifest",
          "/ipwa/*.css",
          "/ipwa/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/ipwa/assets/**",
          "/ipwa/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}
